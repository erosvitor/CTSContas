<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.ctseducare.ctscontas">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_ctscontas"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_ctscontas_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".view.SplashActivity" android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.MainActivity" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".view.AccountEditActivity" />
        <activity android:name=".view.AccountListActivity" />
        <activity android:name=".view.AboutActivity" />
    </application>

</manifest>